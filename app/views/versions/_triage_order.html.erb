<div class="row">
  <div class="col">
    <h3><%= t('questions.categories.first_look_assessment.label') %></h3>
    <ul id="triage_first_look_assessments_order" class="triage-order">
      <% @version.triage_first_look_assessments_order.each do |question_id| %>
        <% question =  Node.find(question_id) %>
        <li class="ui-state-default" value=<%= question_id %>><span class="ui-icon ui-icon-arrowthick-2-n-s"></span><%= question.reference_label %></li>
      <% end %>
    </ul>
  </div>

