<div class="row">
  <% @version.medal_r_config['questions_orders'].each do |key, value| %>
    <div class="col-md-6">
      <h3><%= t("questions.steps.#{key}") %></h3>
      <ul id=<%= "#{key}" %> class="triage-order <%= @version.is_deployed? ? 'order-unavailable' : 'order-available' %>">
        <% value.each do |node_id| %>
          <% node = Node.find(node_id) %>
          <li class="ui-state-default" value=<%= node_id %>><span class="ui-icon ui-icon-arrowthick-2-n-s"></span><%= node.reference_label %></li>
        <% end %>
      </ul>
    </div>
  <% end %>
</div>
