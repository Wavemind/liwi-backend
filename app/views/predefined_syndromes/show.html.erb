<h1><%= t('.title', name: @instanceable.label ) %></h1>
<div class="col text-right">
  <%= link_to(I18n.t('open_diagram'), diagram_predefined_syndrome_url(@predefined_syndrome), class: 'btn btn-primary') %>
</div>

<div class="row">
  <div class="col">
    <%= simple_form_for @instance, url: predefined_syndrome_instances_url(@predefined_syndrome), method: :post do |f| %>
      <div class="row">
        <div class="col">
          <%= f.association :node, collection: @algorithm.nodes.where.not(id: @predefined_syndrome.components.select(:node_id)).order(:reference), label_method: :reference_label, label: false , prompt: t('prompt')%>
        </div>
        <div class="col">
          <%= f.submit t('add'), class: 'btn btn-primary' %>
        </div>
      </div>
    <% end %>
  </div>
</div>

<%= render 'instances/table' %>

