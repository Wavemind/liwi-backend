<div class="row">
  <div class="col-md-12 my-1 text-right">
    <%= link_to(I18n.t('close'), predefined_syndrome_url(params[:id]), class: 'btn btn-danger mr-2') %>
  </div>
</div>
<div class="row">
  <div class="col-md-12">
    <p class="mx-0 my-0"data-type="<%= @predefined_syndrome.class.name %>" data-id="<%= @predefined_syndrome.id %>"></p>
    <%= react_component('Provider', {
      render: 'Diagram',
      context: {
        instanceable: @predefined_syndrome,
        instanceable_type: @predefined_syndrome.class.name,
        questions: @predefined_syndrome.questions_json,
        availableNodes: @predefined_syndrome.available_nodes_json,
        type: 'PredefinedSyndrome'
      }
    }) %>
  </div>
</div>
