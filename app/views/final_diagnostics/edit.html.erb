<h1><%= t('.title') %></h1>

<%= render 'utils/dependency_warning', node: @final_diagnostic %>

<p class="mx-0 my-0 metadata" data-type="<%= @diagnostic.class.name %>" data-id="<%= @diagnostic.id %>" data-version="<%= @diagnostic.version.id %>" data-algorithm="<%= @diagnostic.version.algorithm.id %>" data-final_diagnostic="<%= @final_diagnostic.id %>"></p>

<div class="row justify-content-center mt-5">
  <div class="col-md-8">
    <%= react_component('form/FinalDiagnosticForm', { finalDiagnostic: @final_diagnostic, method: 'update', from: 'rails' }) %>
  </div>
</div>
