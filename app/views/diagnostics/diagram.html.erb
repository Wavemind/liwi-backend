<div class="row">
  <div class="col my-1"><%= render_breadcrumbs %></div>
  <div class="col my-1 text-center"><p><%= @diagnostic.label %></p></div>
  <div class="col my-1 text-right">
    <%= link_to(I18n.t('close'), algorithm_version_diagnostic_url(params[:algorithm_id], params[:version_id], params[:id]), class: 'btn btn-danger mr-2') %>
  </div>
</div>
<div class="row">
  <div class="col-md-12">
    <p class="mx-0 my-0 metadata" data-type="<%= @diagnostic.class.name %>" data-id="<%= @diagnostic.id %>" data-version="<%= @diagnostic.version.id %>"  data-algorithm="<%= @diagnostic.version.algorithm.id %>"></p>
    <%= react_component('Provider', {
    render: 'Diagram',
    context: {
      instanceable: @diagnostic,
      instanceableType: @diagnostic.class.name,
      questions: @diagnostic.questions_json,
      finalDiagnostics: @diagnostic.final_diagnostics_json,
      availableNodes: @diagnostic.available_nodes_json,
      type: 'Diagnostic'
    }
  }) %>
  </div>
</div>
